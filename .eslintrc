{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "indent": "warn",
        "quotes": "warn",
        "sort-imports": "warn",
        "no-duplicate-imports": "warn",
        "semi": "warn"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    }
}